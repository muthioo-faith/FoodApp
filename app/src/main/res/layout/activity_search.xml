<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".search">

    <Button
        android:id="@+id/btnsearch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="36dp"
        android:backgroundTint="#FF5722"
        android:text="Search"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/etcategories"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="34dp"
        android:text="Categories"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnsearch" />

    <ImageView
        android:id="@+id/imgjuic"
        android:layout_width="161dp"
        android:layout_height="156dp"
        android:layout_marginStart="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgbanana"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgbanana"
        app:srcCompat="@drawable/juice" />

    <ImageView
        android:id="@+id/imgbanana"
        android:layout_width="170dp"
        android:layout_height="157dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="197dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgjuic"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/banana" />

    <TextView
        android:id="@+id/etjuice"
        android:layout_width="98dp"
        android:layout_height="52dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:text="Juice"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgjuic" />

    <TextView
        android:id="@+id/etfruits"
        android:layout_width="99dp"
        android:layout_height="30dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="32dp"
        android:text="Fruits"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/etjuice"
        app:layout_constraintTop_toBottomOf="@+id/imgbanana" />

    <TextView
        android:id="@+id/ettop"
        android:layout_width="140dp"
        android:layout_height="59dp"
        android:layout_marginStart="24dp"
        android:text="Top List"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="426dp" />

    <ImageView
        android:id="@+id/etchampaign"
        android:layout_width="177dp"
        android:layout_height="201dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="9dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgmixjuice"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgmixjuice"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/cha" />

    <ImageView
        android:id="@+id/imgmixjuice"
        android:layout_width="207dp"
        android:layout_height="195dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="17dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toEndOf="@+id/etchampaign"
        app:srcCompat="@drawable/jui" />
</androidx.constraintlayout.widget.ConstraintLayout>